public class Kuslar {

    public Kuslar() {

        System.out.println(“Kuslar constructor”);

    }

}

public class AvciKuslar extends Kuslar {

    public AvciKuslar() {

        System.out.println(“AvciKuslar constructor”);

    }

    public static void main(String args[]){

        AvciKuslar avc1 = new AvciKuslar();

    }

}


